{
  "schemaVersion": 1,
  "id": "debugrenderers",
  "version": "${version}",
  "name": "Debug Renderers",
  "description": "DebugRenderers enables Vanilla debug renderers that are normally not accessible.",
  "authors": ["adrian154"],
  "contact": {
    "homepage": "https://bithole.dev/",
    "sources": "https://github.com/adrian154/debug-renderers"
  },
  "license": "MIT",
  "icon": "assets/debugrenderers/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["dev.bithole.debugrenderers.DebugRenderersMod"],
    "client": ["dev.bithole.debugrenderers.DebugRenderersClientMod"]
  },
  "mixins": [
    "debugrenderers.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "fabric-key-binding-api-v1": "*"
  }
